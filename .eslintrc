{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "globals": {
    "__DEV__": true,
    "__SERVER__": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/display-name": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-unused-vars": 1
  }
}